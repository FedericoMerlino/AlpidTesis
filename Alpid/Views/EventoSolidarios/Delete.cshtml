@model Alpid.Models.EventoSolidarios

@{
    ViewData["Title"] = "Eliminar";
}

<html>
<body>
    <h3>¿Está seguro que desea eliminar el evento solidario?</h3>
    <div>
        <h4>Detalle sobre Evento solidario</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Cantidad:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Cantidad)
            </dd>
            <dt class="col-sm-2">
                Concepto:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Concepto)
            </dd>
            <dt class="col-sm-2">
                Ingreso:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Ingreso)
            </dd>
            <dt class="col-sm-2">
                Salida:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Salida)
            </dd>
            <dt class="col-sm-2">
                Total:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Total)
            </dd>
            <dt class="col-sm-2">
                Fecha:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Fecha)
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="EventoSolidarioID" />
            <input type="submit" value="Eliminar" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-info">Cancelar</a>
        </form>
    </div>
</body>
</html>