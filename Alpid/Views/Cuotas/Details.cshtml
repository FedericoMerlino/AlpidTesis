@model Alpid.Models.Cuotas

@{
    ViewData["Title"] = "Detalle cuota";
}
<html>
<body style="background-color:ghostwhite;">
    <h1 style="color:blue">Detalle cuota</h1>
    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Socio:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Socios.Cuit) - @Html.DisplayFor(model => model.Socios.RazonSocial)
            </dd>
            <dt class="col-sm-2">
                Estado:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Estado)
            </dd>
            <dt class="col-sm-2">
                Observación:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Observacion)
            </dd>
            <dt class="col-sm-2">
                Fecha Desde:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaDesde)
            </dd>
            <dt class="col-sm-2">
                Fecha Hasta:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaHasta)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Index" class="btn btn-info">Regresar</a>
    </div>
</body>
</html>