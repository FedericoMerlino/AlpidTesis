@model IEnumerable<Alpid.Models.Cuotas>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaEmicion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Socios)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaPago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaEmicion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Socios.Cuit)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CuotasID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CuotasID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CuotasID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
