@model Alpid.Models.Alquiler

@{
    ViewData["Title"] = "Details";
}
<html>
<body>
    <h1>Details</h1>

    <div>
        <h4>Alquiler</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Fecha Desde:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.FechaDesde)
            </dd>
            <dt class="col-sm-2">
                Observación:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Observacion)
            </dd>
            <dt class="col-sm-2">
                Valor unitario:
            </dt>
           
            <dt class="col-sm-2">
                Socio:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Socios.Cuit)
            </dd>
        </dl>
    </div>
    <div>
        <a asp-action="Index" class="btn btn-info">Regresar</a>
    </div>
</body>
</html>